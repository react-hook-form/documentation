{
  "name": "react-hook-form-website",
  "description": "React hooks for form validation without the hassle.",
  "version": "6.0.1",
  "author": "beier luo",
  "dependencies": {
    "@hookform/devtools": "2.0.0-beta.1",
    "@next/bundle-analyzer": "^13.3.0",
    "little-state-machine": "4.8.0",
    "next": "^13.3.0",
    "next-themes": "^0.2.1",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-github-btn": "1.4.0",
    "react-hook-form": "6.0.0",
    "react-simple-animate": "^3.5.2",
    "react-simple-img": "3.0.0",
    "react-sortablejs": "1.5.1",
    "sortablejs": "1.15.0"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.0.34",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-env": "^7.0.3",
    "eslint": "8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.8.7",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "react-hook-form",
    "form-validation"
  ],
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "dev": "next dev",
    "start": "next start",
    "now-build": "npm run build",
    "prettier": "prettier --write './src/**/*.ts' './src/**/*.tsx' --config ./.prettierrc",
    "lint": "next lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format"
    ]
  }
}
